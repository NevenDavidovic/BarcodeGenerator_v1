<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.barcodegenerator_v1.BarcodeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <FlowPane alignment="CENTER" hgap="20.0" prefHeight="47.0" prefWidth="414.0">
        <Label fx:id="welcomeText"/>
        <Button fx:id="editProfileButton" onAction="#onManageUserButtonClick" text="Uredi profil"/>
    </FlowPane>
    <Button text="Generate Barcode" onAction="#onGenerateBarcodeClick"/>
</VBox>